{
    "name": "dragonlhp/think-layer-multi-app",
    "description": "thinkphp6 multi app support",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "runPauli",
            "email": "dragon_lhp@163.com"
        }
    ],
    "require": {
    	"php": ">=7.1.0",
        "topthink/framework": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "think\\app\\": "src"
        }
    },
    "extra": {
        "think":{
            "services":[
                "think\\app\\Service"
            ]
        }
    },    
    "minimum-stability": "dev",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
